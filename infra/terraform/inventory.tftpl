all:
  hosts:
    ml_host:
      ansible_host: ${ip_address}
      ansible_user: zodiac
      ansible_ssh_private_key_file: ~/.ssh/otus
      ansible_ssh_common_args: '-o StrictHostKeyChecking=no'
      ansible_become: true
      ansible_become_method: sudo
      ansible_become_user: root
      ansible_become_pass: null
